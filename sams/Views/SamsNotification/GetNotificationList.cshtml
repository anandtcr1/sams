                                            }
                                        </td>
                                        <td title="@item.PropertyAddress">@item.PropertyAddressShort</td>
                                        <td>@item.StartDate.ToString("MM-dd-yyyy")</td>
                                        <td>@item.EndDate.ToString("MM-dd-yyyy")</td>
                                        <td>@item.Duration</td>
                                        <td>

