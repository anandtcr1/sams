    var showLease = "";

    var rbtnAcquisition = "";
    var rbtnDispositions = "";
    var rbtnLease = "";

    var disableAcquisition = "";
    var disableDisposition = "";

    var periodType = "";

    if (Model.DiligenceType == 0 || Model.DiligenceType == 1)
