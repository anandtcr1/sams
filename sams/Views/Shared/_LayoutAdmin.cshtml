        function doLogout() {
            if (confirm("Do you want to logout?")) {
                var baseUrl = "@Url.Action("LogoutUser", "Admin")";
                location.href = baseUrl;
            }
        }

        function showLogoutOnTimeout() {
            var baseUrl = "@Url.Action("LogoutUser", "Admin")";
            location.href = baseUrl;
        }

        function getNotificationList() {
            $.ajax({
                url: '@Url.Action("GetTopNotificationList", "SamsNotification")',
                type: "GET",
                success: function (result) {
                    //$("#divList").append(result);

                    $("#divTotalNotifications").append(result);
                }
            });
            $("#divLoading").hide();
        }

        function showNotification() {
            var baseUrl = '@Url.Action("GetNotificationList", "SamsNotification")';
            location.href = baseUrl;
        }

        function resetPassword() {
            if (confirm("Do you want to reset your password?")) {
                var baseUrl = "@Url.Action("SendPasswordLink", "SamsUsers")";
                location.href = baseUrl;
            }
        }

        function numericFilter(evt) {
            evt = (evt) ? evt : window.event;
            var charCode = (evt.which) ? evt.which : evt.keyCode;
            if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                return false;
            }
            return true;
        }

        function markDate(ctrlName) {
            $("#p_MarkStartDate").val('');
            $("#p_Duration").val('');
            $("#p_newDate").val('');
            $("#dateControlName").val(ctrlName);
            $('#modal-calculate_date').modal('show');
        }
    </script>
