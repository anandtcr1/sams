        <div class="modal fade" id="modal-calculate_date">
            <script>
                function calculateDates() {
                    if ($("#p_MarkStartDate").val() == '') {
                        alert("Please Select Date");
                        return;
                    }
                    else if ($("#p_Duration").val() == '') {
                        alert("Please Add Duration");
                        return;
                    }
                    else {
                        $("#divLoading").show();
                        //var strtDate = new Date($("#p_MarkStartDate").val());
                        var strtDate = $("#p_MarkStartDate").val();
                        var dtDuration = $("#p_Duration").val();

                        $.getJSON(`@Url.Action("GetRegionByStateId", "Home")?startDate=${strtDate}&duration=${dtDuration}`, function (data) {
                            $("#p_newDate").val(data);
                        });
                        $("#divLoading").hide();
/*
                        




                        var resultDate = new Date(strtDate);
                        resultDate.setDate(strtDate.getDate() + dtDuration);

                        var datestring = (resultDate.getMonth() + 1) + "-" + resultDate.getDate() + "-" + resultDate.getFullYear();

                        $("#p_newDate").val(datestring);
                        */
                    }
                }

                function copyDateToControl() {

                    if ($("#p_MarkStartDate").val() == '') {
                        alert("Please Select Date");
                        return;
                    }
                    else if ($("#p_Duration").val() == '') {
                        alert("Please Add Duration");
                        return;
                    }
                    else {
                        var dateString = $("#p_newDate").val();

                        var ctrlName = $("#dateControlName").val();
                        if (ctrlName == 'p_DueDiligenceExpairyDate') {
                            $("#p_DueDiligenceExpairyDate").val(dateString);
                        }
                        else if (ctrlName == 'p_DueDiligenceExpairyDate_lease') {
                            $("#p_DueDiligenceExpairyDate_lease").val(dateString);
                        }
                        else if (ctrlName == 'p_DueDiligenceExpairyDate_lease_with_purchase') {
                            $("#p_DueDiligenceExpairyDate_lease_with_purchase").val(dateString);
                        }
                        else if (ctrlName == 'p_DDPExtension') {
                            $("#p_DDPExtension").val(dateString);
                        }
                        else if (ctrlName == 'p_DDPExtension_lease') {
                            $("#p_DDPExtension_lease").val(dateString);
                        }
                        else if (ctrlName == 'p_DDPExtension_lease_with_purchase') {
                            $("#p_DDPExtension_lease_with_purchase").val(dateString);
                        }
                        else if (ctrlName == 'p_ClosingDate') {
                            $("#p_ClosingDate").val(dateString);
                        }
                        else if (ctrlName == 'p_LeaseCommencementDate') {
                            $("#p_LeaseCommencementDate").val(dateString);
                        }
                        else if (ctrlName == 'p_OptionPurchaseDate') {
                            $("#p_OptionPurchaseDate").val(dateString);
                        }

                        $('#modal-calculate_date').modal('hide');
                    }

                }
            </script>

            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">Mark Date</h4>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label for="p_MarkStartDate" class="control-label">Start Date</label>
                                    <input type="text" value="" name="p_MarkStartDate" class="form-control" id="p_MarkStartDate" readonly />
                                    <a href="javascript:clearMarkStartDate()">Clear Date</a>
                                    <input type="hidden" id="dateControlName" />
                                </div>
                                <script>
                                    function clearMarkStartDate() {
                                        $("#p_MarkStartDate").val('');
                                    }
                                </script>

                                <div class="form-group">
