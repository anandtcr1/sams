    function changeStatus() {
        var stateId = $("#selPropertyStatus").val();
        if (stateId == 2) {
            $("#divStatusChangeDate").show();
            $("#lblStatusUpdateDate").html("Under Contract Date *");
        }
        else if (stateId == 3) {
