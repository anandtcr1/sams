    function getSelectedAssetType() {

        var assetTypeId = $("#selAssetTypeNetLease").val();
        
        if (assetTypeId == 1) {
            $("#divAssetType").show();
            $("#divAskingPrice").hide();
            $("#divCapRate").hide();
            $("#divAssetTypeLeaseAndFee").hide();
            $("#divAssetTypeOptionToPrice").hide();
            $("#lblAskingRent").text("Asking Rent");
        }
        else if (assetTypeId == 4) {
            $("#divAssetType").hide();
            //$("#divAskingPrice").hide();
            //$("#divCapRate").hide();
            $("#divAssetTypeLeaseAndFee").show();
            $("#divAssetTypeOptionToPrice").hide();
        }
        else if (assetTypeId == 5) {
            $("#divAssetType").hide();
            //$("#divAskingPrice").hide();
            //$("#divCapRate").hide();
            $("#divAssetTypeLeaseAndFee").hide();
            $("#divAssetTypeOptionToPrice").show();
        }
        else if (assetTypeId == 6) {
            $("#divAssetType").show();
            $("#divAskingPrice").hide();
            $("#divCapRate").hide();
            $("#divAssetTypeLeaseAndFee").hide();
            $("#divAssetTypeOptionToPrice").hide();
            $("#lblAskingRent").text("Rent");
        }
        else {
            $("#divAssetType").hide();
            $("#divAskingPrice").show();
            $("#divCapRate").show();
            $("#divAssetTypeLeaseAndFee").hide();
            $("#divAssetTypeOptionToPrice").hide();
        }
    }

    function getSelectedAssetTypeShoppingCenter() {
        var assetTypeId = $("#selAssetTypeShoppingCenter").val();
        if (assetTypeId == 1 || assetTypeId == 6) {
            $("#divAssetType").show();
            $("#divAskingPrice").hide();
            $("#divCapRate").hide();
