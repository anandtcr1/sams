    function checkClientRepresentation() {
        var selected = document.querySelector('[id="radioIsPropertyListed"]:checked')
        if (selected != null) {
            $("#CheckIfPropertyListed").val("1");
            $("#divClientPropertyListed").show();
        }
        else {
            $("#CheckIfPropertyListed").val("0");
