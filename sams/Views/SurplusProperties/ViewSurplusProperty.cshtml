    string hideDiv = " style=display:none; ";
    if (Model.CheckIfPropertyListed == 1)
    {
        isBroker = "checked";
        hideDiv = "  ";
    }

    var hideStatusDateSection = "display:none;";
    var dateStatusText = "";
    if (Model.SelectedPropertyStatusId == 2 || Model.SelectedPropertyStatusId == 3 || Model.SelectedPropertyStatusId == 4)
